
@{
    ViewData["Title"] = "StudentL2";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>StudentL2</h2>

@model IEnumerable<SampleProject.Models.Register>


<table>
    <tr>
        <td>
            Name
        </td>
        <td>
            Address
        </td>
        <td>
            Gender
        </td>
        <td>
            Birth_Date
        </td>
        <td>
            Course
        </td>
        <td>
            Email
        </td>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Name
            </td>
            <td>
                @item.Address
            </td>
            <td>
                @item.Gender
            </td>
            <td>
                @item.Birth_date
            </td>
            <td>
                @item.Course
            </td>
            <td>
                @item.Email
            </td>
        </tr>
    }

</table>




<br />
<br />
<div class="container">
    <h1 align="center">Table 2</h1>
    <table class="table">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Address</th>
                <th scope="col">Gender</th>
                <th scope="col">DOB</th>
                <th scope="col">Course</th>
                <th scope="col">Email</th>
            </tr>
        </thead>
        <tbody>

            @foreach (var item in Model)
            {
            <tr>
                <th scope="row">@item.Name</th>
                <td>@item.Address</td>
                <td>@item.Gender</td>
                <td>@item.Birth_date</td>
                <td>@item.Course</td>
                <td>@item.Email</td>
                <td><button class="btn btn-primary">Edit</button></td>
            </tr>
            }

        </tbody>
    </table>

</div>

